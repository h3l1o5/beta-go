{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "trailingComma": "es5",
        "bracketSpacing": true
      }
    ],
    "semi": [2, "never"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/first": 0,
    "react/no-string-refs": 0,
    "react/no-find-dom-node": 0,
    "react/forbid-prop-types": 0,
    "no-nested-ternary": 0,
    "no-shadow": 0,
    "consistent-return": 0,
    "func-names": 0,
    "jsx-a11y/href-no-hash": 0
  }
}
